def Reverse(list): 
    list.reverse() 
    return list

list = [] 
num = int(input("Enter the number of elements in list: "))
i=1
while i<=num:
    print("Enter number", i, "in the list")
    x=int(input())
    list.append(x)  
    i=i+1
print("The given list is", list)
print("The reverse of the list is", Reverse(list))



